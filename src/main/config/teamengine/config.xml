<?xml version="1.0" encoding="UTF-8"?>
<!-- Unpack the *-ctl-scripts.zip archive in the TE_BASE/scripts directory -->
<!-- Add this entry to TE_BASE/config.xml (//organization/standard) -->
<organization>
  <name>OGC</name>
  <standard>
    <name>GeoPackage Encoding Standard</name>
    <version>
      <name>${spec-version}</name>
      <suite>
        <namespace-uri>http://www.opengis.net/cite/alpha</namespace-uri>
        <prefix>tns</prefix>
        <local-name>ets-alpha-${version}</local-name>
        <title>Test suite ets-alpha</title>
        <description>Describe scope of testing.</description>
      </suite>
      <revision>
        <name>${version}</name>
        <status>Beta</status>
        <sources>
          <source>alpha/${spec-version}/alpha-suite.ctl</source>
        </sources>
        <webdir>alpha/${spec-version}/web</webdir>
      </revision>
    </version>
  </standard>
</organization>
